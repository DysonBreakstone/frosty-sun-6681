<h2><%= @doctor.name %></h2>
<p><%= "Specialty: #{@doctor.specialty}"%></p>
<p><%= "Education: #{@doctor.education}"%></p>
<p><%= "Hospital: #{@doctor.hospital.name}"%></p>
<section id="patients">
  <h3><%= "Patients:" %></h3>
  <ul>
    <% @doctor.patients.each do |patient| %>
      <li><%= patient.name %></li>
    <% end %>
  </ul>
</section>